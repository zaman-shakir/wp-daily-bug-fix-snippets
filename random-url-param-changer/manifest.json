{
    "manifest_version": 3,
    "name": "Random URL Parameter Changer",
    "version": "1.0",
    "permissions": [
        "webRequest",
        "tabs",
        "scripting",
        "storage",
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_title": "Toggle Random Param"
    }
}
